#quotes
	%nav
		%ul
			%li.active
				标签管理
				%i
			%li
				浏览
				%i
			%li
				生成
				%i
	.tag_list
		.alert.alert-info
			%span.icon-tags
			点击删除标签
		- for tag in @tags		
			%span.label
				%i.num
					= tag[1]
				%span.as_link
					= tag[0]
		
	- for q in @quotes
		.quote{qid: q._id }
			.alert.alert-success
				= q.content
			- if q.tags
				.tags
					- for tag in q.tags
						%i.label
							= tag